{
    "editor.fontSize": 15,
    "editor.lineHeight": 1.5,
    // "editor.formatOnPaste": true,
    "debug.console.fontSize": 14,
    "editor.fontFamily": "Dank Mono",
    "window.zoomLevel": -0.5,
    "editor.rulers": [
        80
    ],
    "breadcrumbs.symbolPath": "off",
    "editor.tabSize": 2,
    "editor.wordWrap": "off",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.excludeGitIgnore": false,
    "search.useGlobalIgnoreFiles": true,
    "search.useIgnoreFiles": true,
    "files.autoSave": "onFocusChange",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "files.eol": "\n",
    "workbench.list.openMode": "doubleClick",
    "workbench.editor.showTabs": "none",
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "explorer.compactFolders": false,
    "editor.renderWhitespace": "all",
    "workbench.list.horizontalScrolling": true,
    "workbench.tree.indent": 15,
    "editor.folding": true,
    "editor.scrollbar.vertical": "auto",
    "editor.overviewRulerBorder": false,
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.glyphMargin": false,
    "security.workspace.trust.untrustedFiles": "open",
    "git.confirmSync": false,
    "problems.decorations.enabled": false,
    "markdown.preview.lineHeight": 1.7,
    "workbench.startupEditor": "none",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            // "source.fixAll": "explicit",
            // "source.organizeImports": "explicit"
        }
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            // "source.fixAll": "explicit",
            // "source.organizeImports": "explicit"
        }
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": [
            // "source.organizeImports",
            // "source.fixAll"
        ]
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            // "source.fixAll": "explicit",
            // "source.organizeImports": "explicit"
        }
    },
    "[json]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.codeActionsOnSave": {
        "source.addMissingImports": "explicit"
    },
    "editor.scrollbar.horizontalScrollbarSize": 5,
    "editor.scrollbar.verticalScrollbarSize": 5,
    "terminal.integrated.fontSize": 15,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.productIconTheme": "fluent-icons",
    "editor.dragAndDrop": false,
    "editor.lightbulb.enabled": "off",
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "workbench.editor.centeredLayoutAutoResize": false,
    "editor.linkedEditing": true,
    "eslint.format.enable": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorCustomizations": {
        "[GitHub Dark High Contrast]": {
            "editorCursor.foreground": "#ffffff"
        },
        "[GitHub Light]": {
            "editorCursor.foreground": "#000000"
        },
        "[Codely Dark]": {
            "editorCursor.foreground": "#ffffff"
        },
        "[Default High Contrast]": {
            "editorIndentGuide.background1": "#ffffff58",
            "editorIndentGuide.activeBackground1": "#ffffff58"
        },
        "textPreformat.background": "#efefef39",
        "editorBracketHighlight.foreground1": "#ffffffb2",
        "editorBracketHighlight.foreground2": "#ffffffb2",
        "editorBracketHighlight.foreground3": "#ffffffb2",
        "editorBracketHighlight.foreground4": "#ffffffb2",
        "editorBracketHighlight.foreground5": "#ffffffb2",
        "editorBracketHighlight.foreground6": "#ffffffb2"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "constant.language",
                    "keyword.control",
                    "keyword.operator.expression",
                    "keyword.other",
                    "keyword.operator.new",
                    "variable.language",
                    "entity.other.attribute-name",
                    "storage"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "storage.type.function.arrow"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Codely Dark]": {
            "textMateRules": [
                {
                    "scope": [
                        "variable.other.constant"
                    ],
                    "settings": {
                        "foreground": "#EBDBB2"
                    }
                },
                {
                    "scope": [
                        "variable.other.constant.property"
                    ],
                    "settings": {
                        "foreground": "#B2CCD6"
                    }
                },
                {
                    "scope": [
                        "meta.tag"
                    ],
                    "settings": {
                        "foreground": "#EBDBB2"
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite"
                    ],
                    "settings": {
                        "foreground": "#EBDBB2"
                    }
                },
                {
                    "scope": [
                        "variable.other.object"
                    ],
                    "settings": {
                        "foreground": "#EBDBB2"
                    }
                },
                {
                    "scope": [
                        "variable.other.property"
                    ],
                    "settings": {
                        "foreground": "#8EC07C"
                    }
                },
                {
                    "scope": [
                        "comment.block.documentation"
                    ],
                    "settings": {
                        "foreground": "#A89984"
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        "foreground": "#A89984"
                    }
                },
                {
                    "scope": [
                        "keyword.operator.assignment.compound",
                        "keyword.operator.assignment",
                        "keyword.operator.logical",
                        "keyword.operator.comparison",
                        "keyword.operator.ternary"
                    ],
                    "settings": {
                        "foreground": "#8EC07C",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "string.template"
                    ],
                    "settings": {
                        "foreground": "#B8BB26"
                    }
                },
                {
                    "scope": [
                        "punctuation.definition.string.template.begin",
                        "punctuation.definition.string.template.end"
                    ],
                    "settings": {
                        "foreground": "#B8BB26"
                    }
                },
                {
                    "scope": [
                        "entity.name.tag"
                    ],
                    "settings": {
                        "foreground": "#8EC07C"
                    }
                }
            ]
        },
        "[One Monokai]": {
            "comments": "#b0b1b0"
        },
        "[monokai-charcoal (gray)]": {
            "comments": "#b0b1b0"
        }
    },
    "remote.SSH.remotePlatform": {
        "sandbox.vs7txy.csb": "linux"
    },
    "terminal.integrated.env.osx": {},
    "editor.cursorBlinking": "solid",
    "editor.accessibilitySupport": "off",
    "editor.cursorSurroundingLines": 5,
    "files.exclude": {
        "*/.next//**": true,
